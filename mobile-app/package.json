{
  "name": "actionladder-mobile",
  "version": "1.0.0",
  "description": "ActionLadder Mobile App - Pool hall management and live tournament streaming",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "generate:assets": "node ./scripts/generate-assets.js",
    "decode:placeholders": "node ./scripts/decode-placeholders.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "preview:android": "eas build --platform android --profile preview",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "update": "eas update",
    "prebuild:assets": "node ./scripts/check-assets.js",
    "prebuild": "npm run prebuild:assets",
    "build:android:production": "npm run prebuild:assets && eas build --platform android --profile production",
    "build:android:preview": "npm run prebuild:assets && eas build --platform android --profile preview",
    "build:ios:production": "npm run prebuild:assets && eas build --platform ios --profile production",
    "build:all:production": "npm run prebuild:assets && eas build --platform all --profile production"
  },
  "dependencies": {
    "expo": "^54.0.30",
    "expo-camera": "~13.4.4",
    "expo-location": "~16.1.0",
    "expo-media-library": "~15.4.1",
    "expo-sharing": "~11.5.0",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "^0.72.17",
    "react-native-webview": "^13.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "keywords": [
    "pool",
    "billiards",
    "tournament",
    "ladder",
    "streaming",
    "gambling",
    "ActionLadder"
  ],
  "author": "ActionLadder Team",
  "license": "MIT",
  "private": true
}
